{
    "name": "GTD Web Client Development Environment",
    "context": "..",
    "dockerFile": "../Dockerfile",
    "runArgs": ["--privileged", "--net", "host"],
    "customizations": {
        "vscode": {
          "extensions": ["Dart-Code.dart-code", "Dart-Code.flutter", "alexisvt.flutter-snippets"]
        }
    },
    "workspaceMount": "source=${localWorkspaceFolder}/workspace,target=/home/developer/workspace,type=bind,consistency=delegated",
    "workspaceFolder": "/home/developer/workspace"
}
